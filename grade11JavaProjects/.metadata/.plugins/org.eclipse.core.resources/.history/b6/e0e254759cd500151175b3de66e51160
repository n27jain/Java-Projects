package competetion;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner sc=new Scanner(System.in);

int win=0;

int x=0;
while(x!=6){
char player=(char) sc.nextByte();
	x++;
if(player=='W'){
	win++;
}
}

if(win>=5){
	System.out.print("1");
}
else if(win>2&&win<5){
	System.out.print("2");
}
else if(win<3&&win>0){
	System.out.print("3");
}
else if(win==0){
	System.out.print("-1");
}
}
		
		
	

}
