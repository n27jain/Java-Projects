import java.util.Scanner;


public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner sc=new Scanner(System.in);
char player=(char) sc.nextByte();
int win;
int lose;
int x=0;
while(x!=6){
	x++;
if(player=='W'){
	win++;
}
else if (player=='L'){
	lose++;

}
if(win>=5){
	System.out.print("1");
}
else if(win>2&&win<5){
	System.out.print("2");
}
else if(win<3&&win>0){
	System.out.print("3");
}
else if(win==0){
	System.out.print("-1");
}
}
		
		
	}

}
